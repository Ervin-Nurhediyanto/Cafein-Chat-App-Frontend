         <div class=\"col-md-8 col-sm-8 contact-info\">\r\n            <div class=\"row contact-name\">\r\n              <h4>{{user.name}}</h4>\r\n            </div>\r\n            <div class=\"row last-chat\">\r\n              <h4>Why did you do that?</h4>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-3 col-sm-3 time-info\">\r\n            <div class=\"row time-chat justify-content-end\">\r\n              <h4>15:30</h4>\r\n            </div>\r\n            <div class=\"row qly-chat justify-content-end\">\r\n              <h4>\r\n                <span>2</span>\r\n              </h4>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row contact-menu\">\r\n          <div class=\"row contact-menu-i justify-content-between\">\r\n            <i class=\"far fa-bookmark\"></i>\r\n            <i class=\"fas fa-check-double\"></i>\r\n            <i class=\"far fa-trash-alt\"></i>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <ContactInfo\r\n  v-show=\"showContactInfo\"\r\n  :infoUser=\"infoUser\"/>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport ContactInfo from './Chat-Contact-Info'\r\nexport default {\r\n  name: 'Chat-Contact',\r\n  props: ['allUser'],\r\n  data () {\r\n    return {\r\n      showContactInfo: true,\r\n      infoUser: null\r\n    }\r\n  },\r\n  components: {\r\n    ContactInfo\r\n  },\r\n  methods: {\r\n    handleShowContact (user) {\r\n      // console.log(user)\r\n      if (this.showContactInfo === false) {\r\n        this.showContactInfo = true\r\n        this.infoUser = user\r\n      } else {\r\n        this.showContactInfo = false\r\n      }\r\n    }\r\n    // starChat (room) {\r\n    //   this.$emit('starChat', room)\r\n    // }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n.contact-chat {\r\n  height: 250px;\r\n  width: 320px;\r\n  padding: 20px;\r\n  padding-left: 11px;\r\n  overflow-y: scroll;\r\n  /* background-color: yellow; */\r\n  z-index: 2