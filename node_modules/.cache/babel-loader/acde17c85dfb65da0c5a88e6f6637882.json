o === false) {\r\n        this.showContactInfo = true\r\n        this.infoUser = user\r\n      } else {\r\n        this.showContactInfo = false\r\n      }\r\n    },\r\n\r\n    handleOpenMessage (user) {\r\n      if (user.idFriend) {\r\n        this.headerMessage = user\r\n        this.$emit('headerMessage', this.headerMessage)\r\n        // alert(user.id)\r\n        this.getPrivateChat(user.id)\r\n      } else {\r\n        this.getNotif('Not in contact')\r\n        this.$emit('openNotif', true)\r\n      }\r\n    },\r\n\r\n    handleClose (handleClose) {\r\n      this.showContactInfo = false\r\n    },\r\n\r\n    handleAddContact (user) {\r\n      if (user.idFriend) {\r\n        this.getNotif('in contact')\r\n      } else if (this.idUser === user.id) {\r\n        this.getNotif(\"can't add contact\")\r\n      } else {\r\n        const data = {\r\n          idUser: this.idUser,\r\n          idFriend: user.id\r\n        }\r\n        this.addContact(data).then((res) => {})\r\n        this.$emit('openNotif', true)\r\n      }\r\n    },\r\n    handleDeleteContact (user) {\r\n      if (user.idFriend) {\r\n        this.deleteContact(user.id).then((res) => {})\r\n        this.$emit('openNotif', true)\r\n      } else {\r\n        this.getNotif('Not in contact')\r\n        this.$emit('openNotif', true)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.mobile {\r\n  display: none;\r\n}\r\n\r\n.contact-chat {\r\n  height: 250px;\r\n  width: 320px;\r\n  padding: 20px;\r\n  padding-left: 11px;\r\n  overflow-y: scroll;\r\n  /* background-color: yellow; */\r\n  z-index: 2;\r\n}\r\n\r\n.contact-chat::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n\r\n.contact-chat .contact-list-chat {\r\n  width: 390px;\r\n  height: 100px;\r\n  padding: 0;\r\n  /* background-color: purple; */\r\n}\r\n\r\n.contact-chat .container-photo {\r\n  width: 20%;\r\n  padding: 0;\r\n}\r\n\r\n.contact-chat .photo {\r\n  width: 100px;\r\n  height: 100px;\r\n  padding: 5px;\r\n  margin-left: 0p